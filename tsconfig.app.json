{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
      "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "ag-grid-community": [
        "../node_modules/ag-grid-community"
      ],
      "ag-grid-community/*": [
        "../node_modules/ag-grid-community/*"
      ]
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
